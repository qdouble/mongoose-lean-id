{
  "name": "mongoose-lean-id-safe",
  "version": "0.2.0",
  "description": "Attach `id` to the results of mongoose queries when using `.lean()` (This fork prevents crash when _id is missing. Also using for loop for optimization.)",
  "main": "index.js",
  "scripts": {
    "docs": "node ./docs",
    "test": "mocha ./test/*.js",
    "test-integration": "mocha ./test/integration.js",
    "test-travis": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qdouble/mongoose-lean-id.git"
  },
  "keywords": [
    "mongoose",
    "lean",
    "id",
    "mongodb"
  ],
  "devDependencies": {
    "acquit": "0.4.1",
    "acquit-ignore": "0.0.3",
    "acquit-markdown": "0.0.5",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mongoose": "4.x"
  },
  "peerDependencies": {
    "mongoose": "4.x"
  },
  "author": "Valeri Karpov <val@karpov.io>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/qdouble/mongoose-lean-id/issues"
  },
  "homepage": "https://github.com/qdouble/mongoose-lean-id"
}
